{
  "name": "@sportup/antd-weekly-calendar",
  "version": "2.1.2",
  "license": "MIT",
  "main": "dist/antd-weekly-calendar.umd.js",
  "module": "dist/antd-weekly-calendar.es.js",
  "typings": "dist/types/index.d.ts",
  "description": "weekly calendar for antd",
  "files": [
    "dist/**/*"
  ],
  "engines": {
    "node": ">=18"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yoavweber/antd-weekly-calendar.git"
  },
  "publishConfig": {
    "@sportup:registry": "https://gitlab.com/api/v4/projects/68163859/packages/npm/"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build ",
    "build:watch": "tsc && vite build --watch",
    "serve": "vite preview",
    "test": "vitest",
    "lint": "eslint . --ext .ts,.tsx",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "author": "Sport'Up",
  "size-limit": [
    {
      "path": "dist/antd-weekly-calendar.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/antd-weekly-calendar.esm.js",
      "limit": "10 KB"
    }
  ],
  "dependencies": {
    "antd": "^6",
    "date-fns": "^3.6.0",
    "less": "^4.2.0",
    "rc-picker": "^4.11.3",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "tslib": "^2.6.3"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^5.1.2",
    "eslint": "^8.45.0",
    "husky": "^9.1.4",
    "identity-obj-proxy": "^3.0.0",
    "jsdom": "^24.1.1",
    "size-limit": "^6.0.0",
    "typescript": "^5.5.4",
    "vite": "^7.3.1",
    "vite-plugin-dts": "^4.2.1",
    "vitest": "^4.0.16"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
